{
  "name": "react-next-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint --dir pages --dir app",
    "prettier:check": "prettier --check \"{app,pages}/**/*.{js,css,scss}\"",
    "prettier:fix": "prettier --write -l \"{app,pages}/**/*.{js,css,scss}\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "i18next": "^21.1.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "4.3.0",
    "eslint-config-next": "^11.1.2",
    "prettier": "2.4.1"
  }
}
